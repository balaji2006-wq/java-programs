import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        if (n <= 0) {
            return;
        }

        int largest = sc.nextInt();

        for (int i = 1; i < n; i++) {
            int num = sc.nextInt();
            if (num > largest) {
                largest = num;
            }
        }

        System.out.println(largest);

        sc.close();
    }
}
